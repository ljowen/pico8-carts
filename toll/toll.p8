pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
g_lane0 = 24

function rnd_idx(arr)
 return flr(rnd(#arr))+1
end

function _init()
 palt(0, true)
	game_over=false
	lanes = {g_lane0,30}
	cars = init_cars()	
	score = 0
	init_menu()
end

function draw_map() 
	map(0,0,0,0,15,15)
end

function draw_lanes()
 function draw_lane(lane)
	 for x=0,14 do
	  if(lane == g_lane0) then
		 	spr(3,x*8,lane) 
	  else
		 	spr(2,x*8,lane) 
	 	end
	 end
 end
 foreach(lanes,draw_lane)
end

function pay()
 score = score + 1
 sfx(0)
end

function _update()
	update_cars()
	if(btnp(4)) then
	 do_menu()
	end
	update_menu()
end

function _draw()
	cls()
	draw_map()
	draw_lanes()
	draw_cars()
 draw_menu()
	print(score, 0, 120, 7)
	print(idx,50,120)
end
-->8
function init_car(lane_idx) 
 local car = {}
 car.x = 0;
 car.paid = false
 car.lane_idx = lane_idx;
 car.dx = 1;
 car.spr = car_sprs[rnd_idx(car_sprs)];
 return car
end

function init_cars(n)
	car_sprs = {1,17,33,49}
 cars = {}
 add_car(1)
 return cars
end

function add_car(lane_idx)
 add(cars, init_car(lane_idx))
end

function update_car(car)
 car.x = car.x + car.dx
 if(car.x >= 8*6 and not car.paid) then
  pay() 
  add_car(rnd_idx(lanes))
  car.paid = true
 end
 if(car.x > 8*15)
 then
 	del(cars, car)
 end
end

function update_cars()
 foreach(cars,update_car)
end

function draw_car(car)
 spr(car.spr,car.x,lanes[car.lane_idx])
end

function draw_cars() 
  foreach(cars,draw_car)
end


-->8
function add_lane()
 printh("adding lane")
 add(lanes, lanes[#lanes]+8)
 score -= 100*#lanes
end

function init_menu() 
 menu = {}
 add(menu,{
 	label="+lane",
 	active=true,
 	action=add_lane})
end

function draw_menu_item(item)
	local c=7
	if(item.active) then c=10 end
	print(tostr(item.label), 0, 112, c)	
end

function draw_menu()
 foreach(menu,draw_menu_item)
end

function update_menu()
 if(#lanes >= 10) then  
 	del(menu, 0) 	
 end
end

function do_menu() 
 for i=1,#menu do
 	if(menu[i].active) then 
 	 menu[i].action()
 	end
 end
end
